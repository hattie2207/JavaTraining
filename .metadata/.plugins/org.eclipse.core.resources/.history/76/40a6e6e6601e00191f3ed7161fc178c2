package LevelTwo.Garage;

import java.util.ArrayList;

public class Vehicle {

	private String ownerName = "";
	private int price = 0;
	private String colour = "";

	public Vehicle(String ownerName, int price, String colour) {
		this.ownerName = ownerName;
		this.price = price;
		this.colour = colour;
	}
	
}
