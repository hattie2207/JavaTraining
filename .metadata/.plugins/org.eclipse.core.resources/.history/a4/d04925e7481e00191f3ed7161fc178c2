
public class Person {
	
	static String name = "";
	static int age = 0;
	static String jobTitle = "";

	public Person(String name, int age, String jobTitle) {
		this.name = name;
		this.age = age;
		this.jobTitle = jobTitle; 			
	}		

	public String toString() {
		return "Name: " + Person.name + " Age: " + Person.age + " Job Title: " + Person.jobTitle;

	}

	public static void main(String[] args) {

		Person p = new Person("N", 13, "J");
		
		p.toString(); 
		
		System.out.println(p.name);

	}

}
