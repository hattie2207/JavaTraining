
public class People {

	public class Person {

		String name = "";
		int age = 0;
		String jobTitle = "";
		
		public Person(String personName, int age, String jobTitle) {
			name = personName; 
			int personAge = age;
			String personJob = jobTitle; 			
		}
		
		

		public String toString() {
			return "Name: " + name + " Age: " + age + " Job Title: " + jobTitle;
			
		}

	}
	
	

	public static void main(String[] args) {
		
	

	}

}
